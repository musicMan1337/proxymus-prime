# Redis Configuration
REDIS_PASSWORD=your_secure_redis_password_here

# Session Configuration
SESSION_TIMEOUT=86400

# Security Settings
RATE_LIMIT_PER_IP=10
RATE_LIMIT_BURST=20

# Keepalived (copy to .env and customize)
KEEPALIVED_VIP=192.168.1.100
KEEPALIVED_INTERFACE=eth0
KEEPALIVED_PASSWORD=your_vrrp_password

# Port Configuration
HTTP_PORT=80
HTTPS_PORT=443
REDIS_PORT=6379

# Backend Test Ports (for test mode)
BACKEND1_PORT=8081
BACKEND2_PORT=8082
BACKEND3_PORT=8083
